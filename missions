// mission 1 
    Console.Write("First Number = ");
    int first = int.Parse(Console.ReadLine());

    Console.Write("Second Number = ");
    int second = int.Parse(Console.ReadLine());

if (first > second)
{
    Console.WriteLine(first);
}  
else if (second > first)
{ Console.WriteLine(second); 
}

// mission 2 

  Console.Write("First Number = ");
    int first = int.Parse(Console.ReadLine());

    Console.Write("Second Number = ");
    int second = int.Parse(Console.ReadLine());
int x = first / second;
int y = second / first;
Console.WriteLine(x);
Console.WriteLine(y);

