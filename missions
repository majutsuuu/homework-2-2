    Console.WriteLine("Eight");
else if (first == 9)
    Console.WriteLine("Nine");
else if (first == 10)
    Console.WriteLine("Ten");
else if (first <= 11 || first >= 0)
    Console.WriteLine("You chose an incorrect number!");
    
    // mission 6 
    
    Console.WriteLine("Please enter your name: ");
Console.ReadLine();
Console.WriteLine("Please enter your salary");
int salary = int.Parse(Console.ReadLine());
double x = salary * 1.1;
double y = salary * 1.05;

if (x < 6000)
    Console.WriteLine("Your new salary is " + x );
else if (y > 6000)
    Console.WriteLine("Your new salary is " +  y);


// mission 7 

Console.Write("Please enter a number: ");
int x = int.Parse(Console.ReadLine());
Console.Write("Please enter a second number :");
int y = int.Parse(Console.ReadLine());
Console.Write("Please enter a third number ");
int z = int.Parse(Console.ReadLine());
if (y > x && z > y)
    Console.WriteLine(y + " " + z);


// mission 8 

Console.Write("Please enter a number: ");
int x = int.Parse(Console.ReadLine());
Console.Write("Please enter a second number: ");
int y = int.Parse(Console.ReadLine());
Console.Write("Please enter a third number: ");
int z = int.Parse(Console.ReadLine());
if (y > x && z < y )
    Console.WriteLine(y);
else if (z > x && z > y)
    Console.WriteLine(z);
else if (x > y && x > z )
    Console.WriteLine(x);
    
// mission 9

Console.Write("Please enter a number between 1-9999: ");
int x = int.Parse(Console.ReadLine());
if (x > 9999 || x <= 0)
    Console.WriteLine("Wrong number ");
else if  (x < 10)
    Console.WriteLine("You chose a single digit number ");
else if (x < 100)
    Console.WriteLine("You chose a two digit number ");
else if (x < 1000)
    Console.WriteLine("You chose a three digit number");
else if (x <= 9999)
    Console.WriteLine("You chose a four digit number");


// mission 10

Console.Write("Please enter a number between 1-9999: ");
int x = int.Parse(Console.ReadLine());
Console.Write("Please enter your height:");
int y = int.Parse(Console.ReadLine());

if (x <= 18 && x >= 14 || x >= 21 && x <= 24)
{
    Console.WriteLine("You are of the right age");
}
else if (x > 18 || x < 14 || x < 21 || x > 24)
{
    Console.WriteLine("You are of the wrong age");
}
if (y >= 182)
    Console.WriteLine("You are high enought! ");
else if (y < 182)
    Console.WriteLine("You are too short! ");
    
    // mission 11
    
    Console.Write("Please enter a number: ");
int x = int.Parse(Console.ReadLine());
Console.Write("Please enter your a second number: ");
int y = int.Parse(Console.ReadLine());
Console.Write("Please enter a third number: ");
int z = int.Parse(Console.ReadLine());

if (x > y && x > z && y > z)
    Console.WriteLine(z + " " + y + " " + x);
else if (x > y && x > z && y < z)
    Console.WriteLine(y + " " + z + " " + x);
else if (y > z && y > x && x > z)
    Console.WriteLine(z + " " + x + " " + y);
else if (y > z && y > x && x < z)
    Console.WriteLine(x + " " + z + " " + y);
else if (z > x && z > y && x > y)
    Console.WriteLine(y + " " + x + " " + z);
else if (z > x && z > y && x < y)
    Console.WriteLine(x + " " + y + " " + z);
    
    // end 
