      Console.Write("First Number = ");
    int first = int.Parse(Console.ReadLine());

if (first > 0)
{
    Console.WriteLine("Positive");
}
else if (first < 0)
    Console.WriteLine("Negative");
else if (first == 0)
    Console.WriteLine("You chose zero! ");
    
    // mission 5 
    
